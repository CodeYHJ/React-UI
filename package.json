{
  "name": "@codeyhj/react-ui",
  "version": "0.0.3",
  "description": "",
  "license": "MIT",
  "title": "Code UI",
  "files": [
    "dist",
    "lib",
    "esm"
  ],
  "keywords": [
    "component",
    "components",
    "framework",
    "react",
    "ui"
  ],
  "typings": "lib/index.d.ts",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "sideEffects": [
    "dist/*",
    "esm/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "scripts": {
    "build:cmjs": "tsc --emitDeclarationOnly --outDir ./lib && gulp -f ./script/gulpFile.js",
    "build:esm": "tsc --emitDeclarationOnly --outDir ./esm && gulp -f ./script/gulpFile.js",
    "build:umd": "cross-env NODE_ENV=lib webpack --config ./config/webpack/webpack.config.lib.js && tsc --emitDeclarationOnly --outDir ./dist",
    "build:doces": "cross-env NODE_ENV=pro webpack --config ./config/webpack/webpack.config.pro.js",
    "build:all": "yarn build:cmjs && yarn build:esm && yarn build:umd",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --config ./config/webpack/webpack.config.dev.js --hot",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "ci": "cross-env NODE_ENV=test JEST_JUNIT_OUTPUT_DIR=./test-results/jest/results.xml jest --config=jest.config.ci.js"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@codeyhj/react-ui": "^0.0.2",
    "@types/axios": "^0.14.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.9",
    "@types/jsdom": "^16.2.3",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.3",
    "@types/webpack": "^4.41.21",
    "@types/webpack-env": "^1.15.2",
    "@types/webpack-merge": "^4.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-less": "^4.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.2.2",
    "jest-junit": "^11.1.0",
    "jsdom": "^16.3.0",
    "less": "^3.12.2",
    "less-loader": "^6.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prism-react-renderer": "^1.1.1",
    "raw-loader": "^4.0.1",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^1.2.1",
    "svg-sprite-loader": "^5.0.0",
    "terser-webpack-plugin": "^3.0.7",
    "through2": "^4.0.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
